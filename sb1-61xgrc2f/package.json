{
  "name": "monorepo",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:docs": "npm run dev --workspace=packages/docs",
    "dev:app": "npm run dev --workspace=packages/app",
    "build": "npm run build --workspaces",
    "build:docs": "npm run build --workspace=packages/docs",
    "build:app": "npm run build --workspace=packages/app"
  }
}
